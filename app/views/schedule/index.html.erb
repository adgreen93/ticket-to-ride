
<h1> Listing Schedules of All Employees. </h1>
<p> For admin view only. </p>

<table>
  <tr>
    <th>Employee</th>
    <th>Hours</th>
  </tr>
 
  <% @schedule.each do |schedule| %>
    <tr>
      <td><%= schedule.employee %></td>
      <td><%= schedule.hours %></td>
      <td><%= link_to 'Show', schedule_path(schedule) %></td>
      <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    </tr>


<% end %>
</table>
